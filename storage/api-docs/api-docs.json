                                                "number": {
                                                    "type": "integer",
                                                    "example": 5
                                                },
                                                "neighborhood": {
                                                    "type": "string",
                                                    "example": "Derby"
                                                },
                                                "city": {
                                                    "type": "string",
                                                    "example": "Bom Jardim"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "example": "2021-12-11T09:25:53.000000Z"
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "example": "2021-12-11T09:25:53.000000Z"
